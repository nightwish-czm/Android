<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >
    
	<ImageView 
	    android:id="@+id/iv_contact_img"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:src="@drawable/admin_inactivated"
	    android:layout_marginTop="8dp"
	    android:layout_marginLeft="8dp"
	    />
	
	<TextView 
	    android:id="@+id/tv_contact_name"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:text="老台"
	    android:textSize="25sp"
	    android:layout_toRightOf="@id/iv_contact_img"
	    android:layout_alignTop="@id/iv_contact_img"
		android:layout_marginLeft="22dp"
	    />
	
	<TextView 
	    android:id="@+id/tv_contact_num"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:text="13413644811"
	    android:textSize="18sp"
	    android:layout_below="@id/tv_contact_name"
	    android:layout_toRightOf="@id/iv_contact_img"
	    android:layout_marginTop="5dp"
		android:layout_marginLeft="22dp"
	    />
    
    
    
</RelativeLayout>
